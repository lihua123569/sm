		</dependency>


		<dependency>
			<groupId>commons-codec</groupId>
			<artifactId>commons-codec</artifactId>
			<version>1.9</version>
		</dependency>
		
		
		<!-- 工具映射表到工程 -->
		<dependency>
	 		<groupId>com.liepaiwang</groupId>
	  		<artifactId>code-generator_mysql</artifactId>
	  		<version>0.0.1-SNAPSHOT</version>
		</dependency>
		
	</dependencies>
	
	
  <build>
    <finalName>sm</finalName>
    <plugins>
    <!-- 静态资源压缩start -->
            <plugin>
				<groupId>com.github.mcheely</groupId>
				<artifactId>requirejs-maven-plugin</artifactId>
				<version>2.0.0</version>
				<executions>
					<execution>
					<phase>process-classes</phase>
					<goals>
					<goal>optimize</goal>
					</goals>
					</execution>
				</executions>
				<configuration>
					<configFile>${basedir}/src/main/webapp/build.js</configFile>
					<optimizerFile>${basedir}/src/main/webapp/r.js </optimizerFile>
					whether or not to process config with maven filters
					<filterConfig>false</filterConfig>
					Skip requirejs optimization if true
					<skip>false</skip>
				</configuration>
			</plugin>
			 <!-- 静态资源压缩end -->
			 
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>2.3.2</version>
        <configuration>
          <source>1.7</source>
          <target>1.7</target>
        </configuration>
      </plugin>
      
      <plugin>
        <artifactId>maven-war-plugin</artifactId>
        <version>2.2</version>
        <configuration>
          <version>3.1</version>
          <failOnMissingWebXml>false</failOnMissingWebXml>
        </configuration>
      </plugin>
            <plugin>
                <groupId>org.apache.tomcat.maven</groupId>
                <artifactId>tomcat7-maven-plugin</artifactId>
                <version>2.1</version>
                <configuration>
                    <port>8091</port>
                    <path>/sm</path>
                    <uriEncoding>UTF-8</uriEncoding>
                    <server>tomcat7</server>
                </configuration>
            </plugin>
             
    </plugins>
    <resources>  
            <resource>  
                <directory>src/main/java</directory>  
                <includes>  
                    <include>**/*.properties</include>  
                    <include>**/*.xml</include>  
                </includes>  
                <!-- 是否替换资源中的属性-->  
                <filtering>false</filtering>  
            </resource>  
            <resource>  
                <directory>src/main/resources</directory>  
            </resource>  
        </resources>
  </build>
  
  
</project>
